APP_NAME=Maestro
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=maestro
DB_USERNAME=sail
DB_PASSWORD=password

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120

CACHE_STORE=redis

REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Sail Configuration
WWWGROUP=1000
WWWUSER=1000
APP_PORT=80
FORWARD_DB_PORT=3306
FORWARD_REDIS_PORT=6379
SAIL_XDEBUG_MODE=off

# Maestro Configuration
MAESTRO_QUEUE_CONNECTION=redis
MAESTRO_QUEUE_NAME=maestro
MAESTRO_ZOMBIE_DETECTION=true
MAESTRO_ZOMBIE_THRESHOLD=30
MAESTRO_LOCK_DRIVER=database
MAESTRO_LOCK_TIMEOUT=30
MAESTRO_MAX_RETRY_ATTEMPTS=3
MAESTRO_RETRY_BACKOFF=60
MAESTRO_RETRY_MULTIPLIER=2.0
MAESTRO_CLEANUP_ENABLED=false
MAESTRO_KEEP_COMPLETED_DAYS=30
MAESTRO_KEEP_FAILED_DAYS=90
